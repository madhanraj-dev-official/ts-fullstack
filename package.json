{
  "name": "kalaimahal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node backend/server.ts",
    "build:server": "tsc",
    "build": "npm run client:build && npm run admin:build && npm run admin:update",
    "client": "cd frontend/kalaimahal && npm run dev",
    "admin": "cd frontend/AdminPanel && npm run dev",
    "client:build": "cd frontend && rm -r client/* && cd kalaimahal && npm run build && cp -r dist/* ../client/",
    "admin:build": "cd frontend && rm -r admin/* && cd AdminPanel && npm run build && cp -r dist/* ../admin/",
    "admin:update": "",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "git": "git add . && git commit -m 'file update'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
